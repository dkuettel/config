if mountpoint /data/sets; then
	echo '/data/sets is already mounted'
else
	sudo mount /dev/xvdf /data/sets
fi

if mountpoint /data/testsets; then
	echo '/data/testsets is already mounted'
else
	sudo mount /dev/xvdg /data/testsets
fi

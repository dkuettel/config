[user]
    name = Daniel Kuettel
[core]
    editor = vim
[push]
    default = simple
[diff]
    tool = vimdiff
[pager]
    branch = false
    stash = false
    config = false
[alias]
    l = log --oneline
    lfp = log --oneline --first-parent
    lg = log --graph --simplify-by-decoration --all --pretty=tformat:'%Cblue%D [%h]%n %Cgreen%ar %Creset%s%n'
    s = status
    b = branch -vv
    mb = branch -vv --list 'dk/*'
    mrb = branch -rv --list 'origin/dk/*'
    co = checkout
    c = commit
    cm = commit -m
    f = fetch
    fa = fetch --all
    fp = fetch --prune
    m = merge
    p = push
    pu = push -u origin HEAD
    sl = stash list
    ss = show stash@{0}
    su = stash push
    sd = stash pop
    cp = cherry-pick
    d = diff
    rc = rebase --continue
    sm = submodule
